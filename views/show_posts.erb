<% if @user %>
  <form action="/log_out" method="get">
    <input type="submit" value="Log-out">
  </form>
  <form action="/posts/new" method="get">
    <input type="submit" value="New post">
  </form>
  <h2> <%= "Welcome #{@user.username}!" %> </h2>
<% end %>

<% @peeps.each do |peep| %>
   <h3> <%= "#{peep.username} : #{peep.content}" %> <h3>
   <h4> <%= "#{peep.created_at}" %> </h4>
   <hr>
<% end %>
